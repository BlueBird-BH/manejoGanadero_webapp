<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletAgregarDatos</servlet-name>
        <servlet-class>co.edu.unisabana.manejoganado.conexion.ServletAgregarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>co.edu.unisabana.manejoganado.conexion.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletObtenerDatos</servlet-name>
        <servlet-class>co.edu.unisabana.manejoganado.conexion.ServletObtenerDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEditarDatos</servlet-name>
        <servlet-class>co.edu.unisabana.manejoganado.conexion.ServletEditarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEliminarDatos</servlet-name>
        <servlet-class>co.edu.unisabana.manejoganado.conexion.ServletEliminarDatos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAgregarDatos</servlet-name>
        <url-pattern>/ServletAgregarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletObtenerDatos</servlet-name>
        <url-pattern>/ServletObtenerDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEditarDatos</servlet-name>
        <url-pattern>/ServletEditarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEliminarDatos</servlet-name>
        <url-pattern>/ServletEliminarDatos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
